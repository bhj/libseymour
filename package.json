{
  "name": "libseymour",
  "version": "0.0.0-dev.0",
  "description": "Library for interacting with the Google Reader/GReader API",
  "type": "module",
  "exports": "./dist/index.js",
  "files": ["dist/*"],
  "repository": {
    "type": "git",
    "url": "https://github.com/bhj/libseymour"
  },
  "scripts": {
    "dev": "webpack serve --static src --open",
    "docs:build": "typedoc",
    "docs:dev": "typedoc --watch",
    "docs:serve": "webpack serve --static docs --open",
    "dist": "tsc && (echo '/*!' && cat LICENSE && echo '*/' && cat ./dist/index.js) | terser --module --compress -o ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "terser": "^5.37.0",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.27.5",
    "typedoc-plugin-merge-modules": "^6.1.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  }
}
